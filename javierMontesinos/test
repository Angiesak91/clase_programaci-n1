import java.util.Scanner;

public static void main(String[] arg){
	Scanner s= new Scanner(System.in);
	int n, base,potencia;
	System.out.println("introduce un valor positivo para la base:"+base);
	base = s.nextInt();
	System.out.println("introduce un valor positivo para el exponente:"+n);
	base = s.nextInt();
	for(int i = 0; i<= n; i++)
	{
		potencia = potencia * base; 
		System.out.println("el valor de: "+base+" elevado a:"+n+"es:"+potencia);
	}	
}
